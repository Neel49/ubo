{
    "version": "0.1.0",
    "description": "One-command uBlock Origin installer for Google Chrome on Windows",
    "homepage": "https://github.com/neel49/ubo",
    "license": "MIT",
    "url": "https://github.com/neel49/ubo/archive/refs/tags/v0.1.0.zip",
    "hash": "",
    "extract_dir": "ubo-0.1.0\\windows",
    "bin": [
        [
            "ubo.ps1",
            "ubo",
            "-NoProfile -ExecutionPolicy Bypass -File \"$dir\\ubo.ps1\""
        ]
    ],
    "notes": [
        "To set up uBlock Origin in Chrome, run:",
        "  ubo install",
        "",
        "Then open 'Chrome uBO' from your Desktop or Start Menu."
    ]
}
